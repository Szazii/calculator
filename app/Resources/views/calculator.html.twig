{% extends 'base.html.twig' %}

{% block body %}
    <div>
        {{ form_start(form) }}
        {{ form_widget(form.Operation, { 'value' : myValue }) }}
        {{ form_end(form) }}
        {% for msg in app.session.flashBag.get('success') %}
            <div class="alert alert-success">
                {{ msg }}
            </div>
        {% endfor %}
    </div>
{% endblock %}